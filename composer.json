{
    "name": "smart/linkedin-php",
    "description": "PHP library for LinkedIn",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "league/oauth2-linkedin": "^4.1",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/serializer": "^3.4",
        "symfony/property-access": "^3.4",
        "symfony/property-info": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "SMarT\\LinkedIn\\": "lib/LinkedIn/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMarT\\LinkedIn\\Test\\": "test/LinkedIn/"
        }
    },
